+function flattenTextContent(value: unknown): string {
+  if (Array.isArray(value)) {
+    return value.map((item) => flattenTextContent(item)).join('');
+  }
+
+  if (value === null || value === undefined || typeof value === 'boolean') {
+    return '';
+  }
+
+  return String(value);
