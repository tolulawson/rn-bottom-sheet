+
+      if (props.isOpen) {
+        props.onWillPresent?.();
+        props.onDidPresent?.();
+        props.onOpenChange?.(true, 'programmatic');
+      } else {
+        props.onWillDismiss?.();
+        props.onDidDismiss?.();
+        props.onOpenChange?.(false, 'programmatic');
+      }
+
+      previousOpenState.current = props.isOpen;
+    }, [
+      props.isOpen,
+      props.onDidDismiss,
+      props.onDidPresent,
